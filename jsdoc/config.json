{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "systemName"      : "Faker",

    "theme"                 : "united",
    "footer"                : "",
    "copyright"             : "Faker doc - Generated with jsduck <br/> DocStrap Copyright 2012-2013 The contributors to the JSDoc3 and DocStrap projects.",
    "navType"               : "vertical",
    "linenums"              : true,
    "collapseSymbols"       : false,
    "inverseNav"            : true,
    "outputSourceFiles"     : true ,
    "outputSourcePath"      : true,
    "dateFormat"            : "d-m-Y hh:mm",
    "highlightTutorialCode" : true
  }
}